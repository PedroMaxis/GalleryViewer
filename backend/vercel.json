{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/upload",
          "dest": "/upload"
        },
        {
          "src": "/items",
          "dest": "/items"
        },
        {
          "src": "/uploads/(.*)",
          "dest": "/uploads/$1"
        }
      ]
  }
  